PROGRAM DataMahasiswa

DEKLARASI
    n, i : integer
    nama[n], indeks : string
    nilai[n] : integer

ALGORITMA
    INPUT n
    
    // Input data
    FOR i = 0 TO n-1
        INPUT nama[i], nilai[i]
    ENDFOR
    
    // Output hasil
    OUTPUT "=== HASIL ==="
    FOR i = 0 TO n-1
        IF nilai[i] >= 85 THEN indeks = 'A'
        ELSE IF nilai[i] >= 70 THEN indeks = 'B'
        ELSE IF nilai[i] >= 55 THEN indeks = 'C'
        ELSE IF nilai[i] >= 40 THEN indeks = 'D'
        ELSE indeks = 'E'
        
        OUTPUT nama[i], nilai[i], indeks
    ENDFOR

END
